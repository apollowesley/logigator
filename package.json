{
	"name": "logigator-editor",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --port 8202 --disableHostCheck --proxy-config proxyconfig.json",
		"build": "npm run wasm-build && ng build --prod",
		"wasm-build": "run-script-os",
		"wasm-build:win32": "copy /Y node_modules\\logigator-simulation\\webAssembly\\dist\\* src\\assets\\wasm",
		"wasm-build:default": "cp node_modules/logigator-simulation/webAssembly/dist/* src/assets/wasm",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^8.2.10",
		"@angular/common": "^8.2.10",
		"@angular/compiler": "^8.2.10",
		"@angular/core": "^8.2.10",
		"@angular/forms": "^8.2.10",
		"@angular/platform-browser": "^8.2.10",
		"@angular/platform-browser-dynamic": "^8.2.10",
		"@angular/router": "^8.2.10",
		"@ngx-translate/core": "^11.0.1",
		"@ngx-translate/http-loader": "^4.0.0",
		"bootstrap": "^4.3.1",
		"file-saver": "^2.0.2",
		"ngx-toastr": "^11.1.0",
		"pixi.js": "^5.1.5",
		"run-script-os": "^1.0.7",
		"rxjs": "~6.4.0",
		"tslib": "^1.10.0",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.803.9",
		"@angular/cli": "^8.3.9",
		"@angular/compiler-cli": "^8.2.10",
		"@angular/language-service": "^8.2.10",
		"@types/emscripten": "^1.38.0",
		"@types/file-saver": "^2.0.1",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "^2.0.8",
		"@types/node": "~8.9.4",
		"codelyzer": "^5.1.2",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"logigator-simulation": "github:logigator/logigator-simulation#development",
		"protractor": "~5.4.0",
		"ts-node": "~7.0.0",
		"tslint": "~5.15.0",
		"typescript": "~3.5.3"
	}
}
