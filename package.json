{
	"name": "logigator-editor",
	"version": "0.0.0",
	"scripts": {
		"start": "npm run start:web",
		"start:web": "env-cmd.cmd -e dev-web --use-shell \"npm run build-custom-build-scripts && npm run ng:serve\"",
		"start:web-prod": "env-cmd.cmd -e prod-web --use-shell \"npm run build-custom-build-scripts && npm run ng:serve -- -c production\"",
		"start:electron": "env-cmd.cmd -e dev-web --use-shell \"npm run build-custom-build-scripts && npm-run-all -p ng:serve electron:serve\"",
		"ng:serve": "ng serve --port 8202 --disableHostCheck --proxy-config proxyconfig.json",
		"electron:serve": "wait-on http-get://localhost:8202/ && npm run electron:tsc && electron . --serve",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"electron:tsc": "tsc -p tsconfig.electron.json",
		"build-custom-build-scripts": "tsc -p custom-build-scripts/tsconfig-custom-build-scripts.json",
		"build:web": "env-cmd -e prod-web --use-shell \"npm run build-custom-build-scripts && ng build --prod \""
	},
	"main": "./dist/electron/main.js",
	"private": true,
	"dependencies": {
		"@angular/animations": "~8.2.3",
		"@angular/common": "~8.2.3",
		"@angular/compiler": "~8.2.3",
		"@angular/core": "~8.2.3",
		"@angular/forms": "~8.2.3",
		"@angular/platform-browser": "~8.2.3",
		"@angular/platform-browser-dynamic": "~8.2.3",
		"@angular/router": "~8.2.3",
		"@ngx-translate/core": "^11.0.1",
		"@ngx-translate/http-loader": "^4.0.0",
		"bootstrap": "^4.3.1",
		"file-saver": "^2.0.2",
		"ngx-toastr": "^11.1.0",
		"pixi.js": "^5.1.2",
		"rxjs": "~6.4.0",
		"tslib": "^1.10.0",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^8.2.0",
		"@angular-devkit/build-angular": "~0.803.0",
		"@angular/cli": "~8.3.0",
		"@angular/compiler-cli": "~8.2.3",
		"@angular/language-service": "~8.2.3",
		"@ngtools/webpack": "^8.3.8",
		"@types/file-saver": "^2.0.1",
		"@types/jasmine": "~3.3.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/jsdom": "^12.2.4",
		"@types/node": "~8.9.4",
		"codelyzer": "^5.0.0",
		"electron": "^6.0.12",
		"electron-reload": "^1.5.0",
		"env-cmd": "^10.0.1",
		"html-loader": "^0.5.5",
		"jasmine-core": "~3.4.0",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.1.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~2.0.1",
		"karma-jasmine-html-reporter": "^1.4.0",
		"npm-run-all": "^4.1.5",
		"protractor": "~5.4.0",
		"tslint": "~5.15.0",
		"typescript": "~3.5.3",
		"wait-on": "^3.3.0",
		"webpack-preprocessor-loader": "github:logigator/preprocessor-loader"
	}
}
